mixin bottom_player()
	.footer_player_holder
		.player_progress_track
			.player_progress
			.player_progress_pin.mob_only
			.player_progress_pos.mob_only 0:23
			.player_progress_len.mob_only 2:23
		a.player_artist.mob_hidden(href='#'): img(src='i/artist_1.jpg')
		.player_control_panel
			.middle_block.player_mob_download: .download_block
				.download_share.mob_hidden -70%
				a.download_price.icon-download(href='#')
					span.mob_hidden $ 1.30
			.middle_block.player_mob_composition
				.composition_author
					p Tommie Sunshine's Megasix Smash-Up
					p
						a.icon-heart.favorite_author.mob_hidden(href='#')
						a.artist_name.mob_hidden(href='#') Pink Floyd,
						span.mob_only ,
						span.middle_block &nbsp; 1972

		.player_controls.player_btn_blue
			.middle_block.mob_hidden: .player_track_time 01:23 / 4:21
			.middle_block.mob_hidden: button.player_btn.icon-player_prev.player_btn_shadow
			.middle_block.play_mob_block: button.player_btn.icon-player_play.player_btn_shadow
			.middle_block.mob_hidden: button.player_btn.icon-player_next.player_btn_shadow
			.middle_block.hoverHolder.playlist_mob_block
				.player_queue_block_corner.hoverBlock.mob_hidden
				.player_queue_block.hoverBlock(style_='display:block !important')
					.player_q_title_holder
						.fl: .middle_block: .player_q_title Player Queue
						.fr: .middle_block
							a.player_q_clear.playerClearBtn(href='#')
								span.mob_hidden Clear queue

					.scrollBar(data-max_screen_height = '50%', data-scroller-class='scrollbar-inner scroller_holder h_380')
						//+footer_playlist()
						+album_table()

					.playlist_controls.mob_hidden
						.fl: .middle_block
							a.btn_v1.playlist_download_btn(href='#') Download selected
						.fl: .middle_block Total size: 29.15 Mb

				button.player_btn.player_list.hoverControl.icon-playlist
			.middle_block.hoverHolder.volume_mob_block
				button.player_btn.player_close_btn.mob_only.playerCloseBtn
				.volume_block.volumeControl.mob_hidden
