		+aside_right()
	
	- if (index_page_login)
		+aside_right()

	.head_search_holder
		- if (genres_panel)
			.genre_control_panel
				.fl: .middle_block
					a.main_menu_toggle_btn.icon-corner_left.genreSwitchBackBtn(href='#')
