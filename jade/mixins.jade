mixin page_header(page_caption, tab_block, filter_aside, sort_block, fav_block, search_context, reg_btns, index_page, index_page_login, in_history, genres_panel)
	//.wrapper_overlay
	.desktop_warning.text_center
		p Visit this page with mobile device, please
		p: a.default_link(href='http://naverstay.me/i/folio/ivave/index.html') desktop version
	div.head_nav_holder
		.nav_block
			.logo_holder: a.logo(href='#'): img(src='i/logo.png')
			+top_menu()
			+tablet_menu()
			- if (reg_btns)
				ul.user_menu
					li.user_m_item: a.btn_v7.user_menu_btn.brown_btn(href='#'): span Login
					li.user_m_item: a.btn_v7.user_menu_btn.blue_btn_v4(href='#'): span Register Now
